{
  "name": "mdzjs",
  "version": "0.18.0",
  "description": "markdown preprocessor for zikojs",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=development rollup --config rollup.config.js",
    "watch": "cross-env NODE_ENV=development rollup --config rollup.config.js -w",
    "build": "cross-env NODE_ENV=production rollup --config rollup.config.js"
  },
  "types": "./types/index.d.ts",
  "exports": {
    ".": "./src/index.js",
    "./*": "./src/loaders/*/index.js"
  },
  "author": "zakaria elalaoui",
  "keywords": [
    "zikojs",
    "zikojs-addons",
    "markdown",
    "remark",
    "mdx",
    "preprocessor",
    "astro-integration",
    "withastro"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakarialaoui10/mdzjs.git"
  },
  "license": "MIT",
  "dependencies": {
    "acorn": "^8.15.0",
    "highlight.js": "^11.11.1",
    "htmlparser2": "^10.1.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-mdx": "^3.1.1",
    "remark-parse": "^11.0.0",
    "remark-toc": "^9.0.0",
    "rollup": "^4.57.1",
    "unified": "^11.0.5",
    "vfile-matter": "^5.0.1",
    "vfile-reporter": "^8.1.1",
    "ziko": "^0.66.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "cross-env": "^10.1.0"
  }
}
